{
  "debug": false,
  "isHeadless": true,
  "scrollToBottom": false,
  "proxies": [],
  "waitUntill": "domcontentloaded",
  "blockResourceTypes": ["image","media","stylesheet","font","other"],
  "whiteListUrls": [
    "https://www.koton.com",
    "https://koton.akinoncdn.com"
  ],
  "blockUrls": [
    ".svg",
    "https://www.koton.com/baskets/basket/"
  ],
  "container": "div.product-info",
  "notInStockSizes": {
    "attribute": "data-value",
    "regex": "",
    "groups": [],
    "selector": "div.variant__options > div.variant__option.-disabled"
  },
  "inStockSizes": {
    "attribute": "data-value",
    "regex": "",
    "groups": [],
    "selector": "div.variant__options > div.variant__option:not(.-disabled)"
  },
  "clickSize": "",
  "title": {
    "attribute": "",
    "regex": "",
    "groups": [],
    "selector": "h1.product-info__header-title"
  },
  "category": {
    "attribute": "",
    "regex": "",
    "groups": [],
    "selector": "//div[@class='breadcrumb ']/a[last()] | //div[@class='breadcrumb ']/a[last()-1]"
  },
  "price": {
    "attribute": "",
    "regex": "",
    "groups": [],
    "selector": "div.price > div.price__base > div.price__price"
  },
  "color": {
    "attribute": "label",
    "regex": "",
    "groups": [],
    "selector": "//pz-variant-option[contains(@class,'-selected')] | //pz-variant-option[not(contains(@class,'-selectable'))]"
  },
  "images": {
    "attribute": "data-src",
    "regex": "",
    "groups": [],
    "selector": "div.product-media__images > picture > img"
  }

}